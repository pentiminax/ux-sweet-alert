{
  "name": "@pentiminax/ux-sweetalert",
  "description": "SweetAlert2 integration for Symfony",
  "license": "MIT",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/controller.js",
  "symfony": {
    "controllers": {
      "sweetalert": {
        "main": "dist/controller.js",
        "webpackMode": "eager",
        "fetch": "eager",
        "enabled": true,
        "autoimport": {
          "sweetalert2/themes/bootstrap-4.css": false,
          "sweetalert2/themes/bootstrap-5.css": false,
          "sweetalert2/themes/bulma.css": false,
          "sweetalert2/themes/material-ui.css": false
        }
      }
    },
    "importmap": {
      "@hotwired/stimulus": "^3.0.0",
      "@hotwired/turbo": "^8.0.0",
      "sweetalert2": "^11.22.0"
    }
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.0.0",
    "@hotwired/turbo": "^8.0.0",
    "sweetalert2": "^11.22.0"
  },
  "devDependencies": {
    "@hotwired/stimulus": "^3.0.0",
    "@hotwired/turbo": "^8.0.0",
    "sweetalert2": "^11.22.0"
  }
}